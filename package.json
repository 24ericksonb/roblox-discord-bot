{
  "name": "roblox-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot for early recruiting server.",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node scripts/clean-dist.js && node dist/index.js",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'tsc && node scripts/clean-dist.js && node dist/index.js'",
    "update": "tsc && node scripts/clean-dist.js && node scripts/deploy-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/24ericksonb/roblox-discord-bot.git"
  },
  "author": "Bryce Erickson <berickson@roblox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/24ericksonb/roblox-discord-bot/issues"
  },
  "homepage": "https://github.com/24ericksonb/roblox-discord-bot#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "install": "^0.13.0",
    "npm": "^10.2.3"
  }
}
